{
  "name": "accessible-drag-and-drop",
  "version": "0.1.0",
  "private": true,
  "description": "This project is meant to teach software engineers how to create drag and drop functionality in React. It includes good examples and bad examples.",
  "homepage": "http://tylerhawkins.info/accessible-drag-and-drop/build/",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,md}\"",
    "format-watch": "onchange \"src/**/*.{js,jsx,md}\" -- prettier --write {{changed}}",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "array-move": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-sortable-hoc": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "react-scripts": "5.0.1"
  }
}
